package delivery;

public class Dish {
	private String name;
	private float price;

	public Dish(String nameDish, float price) {
		this.name = nameDish;
		this.price = price;
	}

	public String getName() {
		return name;
	}


	public float getPrice() {
		return price;
	}

	public boolean inRange(float minPrice, float maxPrice) {
		return (price >= minPrice && price <= maxPrice);
	}
	
	
}
